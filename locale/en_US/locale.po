msgid "plugins.importexport.articleImporter.tiffWarning"
msgstr "A tiff image was found on the HTML file {$file}, and it's not web friendly, convert it to a web format"

msgid "plugins.importexport.articleImporter.warningMissingFile"
msgstr "The HTML file {$file}, references a file {$missingFile}, which wasn't found"

msgid "plugins.importexport.articleImporter.warningMissingAnchor"
msgstr "Anchor ID not found: {$anchor}"
